# popseg de configuração para n8n
# Altere conforme necessário para produção

# URL base do n8n
WEBHOOK_URL=https://n8n.pixseguros.com.br
# Porta padrão
PORT=5678
# Banco de dados Postgres
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=pix_postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=pix
DB_POSTGRESDB_USER=pix
DB_POSTGRESDB_PASSWORD=secret
# Redis (opcional, se usar cache)
# QUEUE_BULL_REDIS_HOST=pix_redis
# QUEUE_BULL_REDIS_PORT=6379
# Autenticação básica (opcional)
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=senha_forte
# Outras variáveis podem ser adicionadas conforme necessário
